  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.28.0",
    "eslint-plugin-jest": "^28.12.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "netlify-cli": "^21.5.0",
    "nock": "^14.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "netlify/functions/**/*.js",
      "!netlify/functions/**/__tests__/**"
    ]
  }
